<Settings>
  <PropertyCategory Name="Operating System" >
    <PropertyGroup Name="Wireless">
      <Property DisplayName="Wireless Enabled" TargetProperty="HardwareConfiguration[HardwareConfiguration]"
                TargetName="HardwareInitData" TargetType="LayoutItem" PossibleValues="0,1"
                Notification="Set to 1 for enabled, 0 for disabled"/>
      <Property DisplayName="Wireless Enabled (Clock)" TargetProperty="HardwareConfiguration[ClockEnable]"
                TargetName="HardwareInitData" TargetType="LayoutItem" PossibleValues="44,60"
                Notification="Set to 60 for enabled, 44 for disabled"/>
      <Property DisplayName="Wireless Mode" TargetProperty="Os.WirelessMode"
                Notification="Controls the behaviour of Flight Mode"
                TargetName="WirelessConfiguration" TargetType="BlackboardComponent"/>
    </PropertyGroup>
